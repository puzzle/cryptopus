{{ yield }}
<PowerSelectMultipleWithCreate
    @options={{@options}}
    @placeholder={{@placeholder}}
    @selected={{this.selectedOptions}}
    @onChange={{this.updateSetting}}
    @onCreate={{this.addSetting}}
    @searchField="label"
    @showCreateWhen={{this.hideCreateOptionOnInvalidIpOrSameName}}
    as |ip|>
  {{ip.label}}
</PowerSelectMultipleWithCreate>
