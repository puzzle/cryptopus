{{ yield }}
<PowerSelectMultipleWithCreate
    @options={{@options}}
    @placeholder={{@placeholder}}
    @selected={{this.selectedOptions}}
    @onChange={{fn this.updateSetting}}
    @onCreate={{fn this.addSetting}}
    @searchField="label"
    @showCreateWhen={{fn this.hideCreateOptionOnInvalidIpOrSameName}}
    as |ip|>
  {{ip.label}}
</PowerSelectMultipleWithCreate>
