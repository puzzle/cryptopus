<table class='table table-stripped'>
  <tr>
    <th>Teams</th>
    <th></th>
    <th></th>
  </tr>
  {{#each .}}
    <tr class='team_row'>
      <td>{{attributes.name}}</td>
      <td><div style='visibility:hidden' id='last_teammember_team_id'>{{id}}</div></td>
      <td class='action_icon'>
        <a data-last-team-id={{id}} id='delete_last_teammember_team_link' href='#'>
          <%= image_tag("remove.svg", class:"result-type-icon", alt:"Remove" ) %>
        </a>
      </td>
    </tr>
  {{/each}}
</table>

